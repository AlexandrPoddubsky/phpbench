<?php

require(__DIR__ . '/../vendor/autoload.php');
$configuration = new PhpBench\Configuration();
$configuration->setPath(__DIR__ . '/../tests/Functional/benchmarks');
$configuration->addReport(array(
    'name' => 'console_table',
    'time_format' => 'integer',
    'revolutions' => true,
    'memory' => true,
    'memory_inc' => true,
));

return $configuration;
