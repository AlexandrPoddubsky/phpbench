{
    "classes": {
        "deviation": [
            [ "number", { "decimal_places": 2 }],
            [ "printf", { "format": "%s%%" } ],
            [ "balance" ]
        ],
        "time": [
            [ "number", { "decimal_places": 0 }],
            [ "printf", { "format": "%s<comment>\u03bcs</comment>" }]
        ]
    },
    "rows": [
        {
            "cells": {
                "subject": {
                    "expr": "string(ancestor-or-self::subject/@name)"
                },
                "elements": {
                    "expr": "string('{{ row.item }}')"
                },
                "time": {
                    "class": "time",
                    "expr": "average(.//iteration[@revs='{{ row.item }}']/@time)"
                },
                "deviation": {
                    "class": "deviation",
                    "expr": "deviation(min(//cell[@name='time']), number(./cell[@name='time'])) div number(./cell[@name='elements'])",
                    "pass": 1
                }
            },
            "with_items": ["10", "100", "1000"],
            "with_query": "//subject[group/@name='array_keys']"
        }
    ],
    "sort": {"subject": "asc"}
}
