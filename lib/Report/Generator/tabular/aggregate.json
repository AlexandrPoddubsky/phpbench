{
    "params": {
        "selector": "//subject/variant"
    },
    "includes": [
        [ "classes.json", ["classes"] ]
    ],
    "rows": [
        {
            "group": "body",
            "cells": [
                {
                    "name": "benchmark",
                    "expr": "class_name(string(ancestor-or-self::benchmark/@class))"
                },
                {
                    "name": "subject",
                    "expr": "string(ancestor-or-self::subject/@name)"
                },
                {
                    "name": "group",
                    "expr": "string(join_node_values(',', ancestor-or-self::subject/group/@name))"
                },
                {
                    "class": "params",
                    "name": "params",
                    "expr": "parameters_to_json(ancestor-or-self::variant/parameter)"
                },
                {
                    "name": "revs",
                    "expr": "sum(descendant-or-self::iteration/@revs)"
                },
                {
                    "name": "its",
                    "expr": "count(descendant-or-self::iteration)"
                },
                {
                    "name": "rej",
                    "expr": "sum(descendant-or-self::iteration/@rejection-count)"
                },
                {
                    "name": "time",
                    "class": "time",
                    "expr": "sum(descendant-or-self::iteration/@time) div sum(descendant-or-self::iteration/@revs)"
                },
                {
                    "name": "memory",
                    "class": "memory",
                    "expr": "average(descendant-or-self::iteration/@memory)"
                },
                {
                    "name": "deviation",
                    "class": "deviation",
                    "expr": "deviation(min(//cell[@name='time']), number(./cell[@name='time']))",
                    "pass": 1
                },
                {
                    "name": "stability",
                    "class": "stability",
                    "expr": "100 - deviation(min(descendant::iteration/@time), max(descendant::iteration/@time))"
                }
            ],
            "with_query": "{{ param.selector }}"
        }
    ]
}
